{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@/components": ["src/components/*"],
      "@/icons": ["src/icons/*"],
      "@/lib": ["src/lib/*"],
      "@/music": ["dist/music/*"],
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
